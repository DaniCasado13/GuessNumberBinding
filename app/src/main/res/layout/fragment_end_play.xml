<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.EndPlayFragment">

    <TextView
        android:id="@+id/tvFinalMessage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="@dimen/tvFinalMessagelayoutWidth"
        android:layout_height="@dimen/tvFinalMessagelayoutHeight"
        android:layout_marginTop="@dimen/tvFinalMessageMargintop"
        android:text="@string/tvFinalMessage" />

    <Button
        android:id="@+id/btnVolver"
        android:text="@string/btnVolver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/btnvolvermargintop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFinalMessage">

    </Button>
</androidx.constraintlayout.widget.ConstraintLayout>